{
  "name": "uic-inventory",
  "version": "1.0.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "format": "prettier --write src --ignore-path ./.gitignore",
    "format:fix": "prettier --loglevel warn --write \"./**/*.{js,jsx,css,md,json}\" --ignore-path ./.gitignore",
    "fix": "npm run format:fix && npm run lint:fix",
    "lint": "eslint ./src --ext .jsx,.js --ignore-path ./.gitignore",
    "lint:fix": "eslint ./src --ext .jsx,.js --quiet --fix --ignore-path ./.gitignore",
    "serve": "vite preview",
    "test": "vitest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@agrc/helpers": "^1.3.1",
    "@arcgis/core": "^4.28.6",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^3.3.2",
    "@tanstack/react-query": "^5.4.3",
    "@tippyjs/react": "^4.2.6",
    "clsx": "^2.0.0",
    "downshift": "^8.2.2",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-storybook": "^0.6.15",
    "immer": "^10.0.3",
    "ky": "^1.1.3",
    "lodash.throttle": "^4.1.1",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-packagejson": "^2.4.6",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "react": "^18.2.0",
    "react-content-loader": "^6.2.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-error-boundary": "^4.0.11",
    "react-hook-form": "^7.47.0",
    "react-phone-number-input": "^3.3.7",
    "react-router-dom": "^6.18.0",
    "react-table": "^7.8.0",
    "react-toastify": "^9.1.3",
    "use-immer": "^0.9.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@hookform/devtools": "^4.3.1",
    "@storybook/addon-actions": "^7.5.2",
    "@storybook/addon-essentials": "^7.5.2",
    "@storybook/addon-links": "^7.5.2",
    "@storybook/react": "^7.5.2",
    "@tailwindcss/forms": "^0.5.6",
    "@tanstack/react-query-devtools": "^5.4.3",
    "@testing-library/react": "^14.0.0",
    "@vitejs/plugin-react-swc": "^3.4.1",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "babel-preset-react-app": "^10.0.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "happy-dom": "^12.10.3",
    "postcss": "^8.4.31",
    "pre-commit": "^1.2.2",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.5",
    "vite": "^4.5.0",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.34.6"
  },
  "pre-commit": "fix",
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports",
      "prettier-plugin-packagejson",
      "prettier-plugin-tailwindcss"
    ],
    "semi": true,
    "singleQuote": true,
    "printWidth": 120,
    "tailwindFunctions": [
      "clsx"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "jest": true,
      "node": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "plugins": [
      "react",
      "jsx-a11y",
      "react-hooks",
      "react-refresh",
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime",
      "plugin:react-hooks/recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:storybook/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ],
      "react-refresh/only-export-components": [
        "warn"
      ],
      "react/prop-types": [
        "off"
      ]
    }
  }
}
