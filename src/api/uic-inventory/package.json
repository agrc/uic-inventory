{
  "name": "uic-inventory",
  "version": "1.0.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "format": "prettier --write src --ignore-path ./.gitignore",
    "format:fix": "prettier --loglevel warn --write \"./**/*.{js,jsx,css,md,json}\" --ignore-path ./.gitignore",
    "fix": "npm run format:fix && npm run lint:fix",
    "lint": "eslint ./src --ext .jsx,.js --ignore-path ./.gitignore",
    "lint:fix": "eslint ./src --ext .jsx,.js --quiet --fix --ignore-path ./.gitignore",
    "serve": "vite preview",
    "test": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@agrc/helpers": "^1.3.1",
    "@arcgis/core": "^4.31.6",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@tanstack/react-query": "^5.62.11",
    "@tanstack/react-table": "^8.20.6",
    "@tippyjs/react": "^4.2.6",
    "clsx": "^2.1.1",
    "downshift": "^9.0.8",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-storybook": "^0.11.1",
    "immer": "^10.1.1",
    "ky": "^1.7.4",
    "lodash.throttle": "^4.1.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.6",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "react": "^18.3.1",
    "react-content-loader": "^7.0.2",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.5",
    "react-error-boundary": "^5.0.0",
    "react-hook-form": "^7.54.2",
    "react-phone-number-input": "^3.4.10",
    "react-router-dom": "^7.1.1",
    "react-toastify": "^11.0.2",
    "use-immer": "^0.11.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.3",
    "@hookform/devtools": "^4.3.3",
    "@storybook/addon-actions": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/addon-mdx-gfm": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/blocks": "^8.4.6",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/test": "^8.4.6",
    "@tailwindcss/forms": "^0.5.9",
    "@tanstack/react-query-devtools": "^5.62.11",
    "@testing-library/react": "^16.1.0",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "babel-preset-react-app": "^10.0.1",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "happy-dom": "^16.3.0",
    "postcss": "^8.4.49",
    "pre-commit": "^1.2.2",
    "prettier": "^3.4.2",
    "prop-types": "^15.8.1",
    "storybook": "^8.4.6",
    "tailwindcss": "^3.4.17",
    "vite": "^6.0.7",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^2.1.8"
  },
  "pre-commit": "fix",
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports",
      "prettier-plugin-packagejson",
      "prettier-plugin-tailwindcss"
    ],
    "semi": true,
    "singleQuote": true,
    "printWidth": 120,
    "tailwindFunctions": [
      "clsx"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "jest": true,
      "node": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "plugins": [
      "react",
      "jsx-a11y",
      "react-hooks",
      "react-refresh",
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime",
      "plugin:react-hooks/recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:storybook/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ],
      "react-refresh/only-export-components": [
        "warn"
      ],
      "react/prop-types": [
        "off"
      ]
    }
  }
}
