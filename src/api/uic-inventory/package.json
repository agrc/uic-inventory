{
  "name": "uic-inventory",
  "version": "1.0.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "format": "prettier --write src --ignore-path ./.gitignore",
    "format:fix": "prettier --loglevel warn --write \"./**/*.{js,jsx,css,md,json}\" --ignore-path ./.gitignore",
    "fix": "npm run format:fix && npm run lint:fix",
    "lint": "eslint ./src --ext .jsx,.js --ignore-path ./.gitignore",
    "lint:fix": "eslint ./src --ext .jsx,.js --quiet --fix --ignore-path ./.gitignore",
    "serve": "vite preview",
    "test": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@agrc/helpers": "^1.3.1",
    "@arcgis/core": "^4.21.2",
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "@hookform/error-message": "^2.0.0",
    "@hookform/resolvers": "^2.8.3",
    "@tippyjs/react": "^4.2.6",
    "clsx": "^1.1.1",
    "downshift": "^6.1.7",
    "ky": "^0.28.7",
    "react": "^17.0.2",
    "react-content-loader": "^6.0.3",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.4.2",
    "react-hook-form": "^7.20.2",
    "react-phone-number-input": "^3.1.41",
    "react-query": "^3.33.4",
    "react-router-dom": "^5.3.0",
    "react-table": "^7.7.0",
    "react-toastify": "^8.1.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@nabla/vite-plugin-eslint": "^1.3.4",
    "@storybook/addon-actions": "^6.4.0-alpha.35",
    "@storybook/addon-essentials": "^6.4.0-alpha.35",
    "@storybook/addon-links": "^6.4.0-alpha.35",
    "@storybook/react": "^6.4.0-alpha.35",
    "@tailwindcss/forms": "^0.3.4",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27.3.1",
    "postcss": "^8.3.11",
    "pre-commit": "^1.2.2",
    "prettier": "^2.4.1",
    "tailwindcss": "^2.2.19",
    "vite": "^2.6.14",
    "vite-react-jsx": "^1.1.2"
  },
  "pre-commit": "fix",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "env": {
      "browser": true,
      "jest": true
    },
    "rules": {
      "react/jsx-uses-react": "off",
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off",
      "prettier/prettier": "off"
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:prettier/recommended",
      "prettier"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ]
      }
    }
  }
}
